{
    "name": "Currency Exchange",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Extension to convert currencies quickly",
    "action":{
        "default_icon": "coin.png",
        "default_popup": "index.html"
    },"permissions":[
        "https://api.api-ninjas.com/*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://ajax.googleapis.com 'unsafe-inline'; object-src 'self'"
}